<?xml version="1.0" encoding="utf-8"?>
<TableLayout android:id="@+id/Table" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:stretchColumns="0,1,2,3"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<TableRow android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<Button android:id="@+id/ViewButton" android:text="@string/ViewButton" />
		<Button android:id="@+id/CreateButton" android:text="@string/CreateButton" />
		<Button android:id="@+id/UpdateButton" android:text="@string/UpdateButton" />
		<Button android:id="@+id/ExitButton" android:text="@string/ExitButton" />
	</TableRow>
</TableLayout>